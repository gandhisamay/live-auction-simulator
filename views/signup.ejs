        @import url('https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');
    </style>
    <link rel="stylesheet" href="style.css">
</head>

<body>

    <h1>Auctinator</h1>

    <div class="signup">
        <form action="http://localhost:8080/signup" method="post">
            <h3>Enter a username:</h3>
            <!-- <label for="username" >Username</label> -->
            <p> <input type="text" name="username" id="username" placeholder="Username" required></p>
            <h3>Enter a password:</h3>
            <!-- <label for="password">Password</label> -->
            <p><input type="password" name="password" id="password" placeholder="Password" required minlength="8"
                    maxlength="13"></p>
            <p class="condition">(8-13 letters)</p>
            <button id="signup">Create Account</button>

        </form>
    </div>

